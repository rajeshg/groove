{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "ignorePatterns": [
    "dist",
    "build",
    "node_modules",
    ".git",
    ".vscode",
    ".idea",
  ],
}
